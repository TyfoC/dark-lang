@specinc "limits.dark"

@set VAR_NAME	1 + 3 * 99;
@set FUN_NAME(a, b, c) {
	(a + VAR_NAME) - c * b
}

@if (!@exist VAR_NAME) { VAR_NAME; }
@else <! IF FALSE !> { 0xDEADDEAD; }

<!
	sval_t main() - entry point
	sval_t - builtin type
!>
sval_t main() {
	return (FUN_NAME(123 - (456 + 789), 993 * 37, -23 + 77 * -99) + -111);	<! return value from function !>
}